# Production Environment
# This will be created on the server by server-setup.sh

# Database
POSTGRES_DB=quizdb
DB_USERNAME=produser
DB_PASSWORD=your-secure-password-here

# API
API_SECRET=prod-secret-456

# Spring settings
JPA_DDL_AUTO=update
JPA_SHOW_SQL=false

# Frontend path (production uses deployed files)
FRONTEND_PATH=./frontend

# Use pre-built image from Docker Hub
DOCKER_IMAGE=nghood/nghood-backend:latest

# SSL
NGINX_SSL_PORT=443
SSL_CERTS=./nginx/ssl