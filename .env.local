# Local Development Environment
# Copy this to .env for local development

# Database
POSTGRES_DB=quizdb_dev
DB_USERNAME=dev
DB_PASSWORD=dev

# API
API_SECRET=dev-secret-123

# Spring settings
JPA_DDL_AUTO=create-drop
JPA_SHOW_SQL=true

# Frontend path (local uses docs folder)
FRONTEND_PATH=./docs

# Build from local source
# Leave DOCKER_IMAGE unset to trigger local build